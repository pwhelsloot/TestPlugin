{
  "name": "test-plugin-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "*********RUN********": "*******************",
    "start": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --publicHost=http://localhost:4700/",
    "*********BUILD********": "*******************",
    "build": "ng build",
    "build-high-mem": "node --max_old_space_size=10000 node_modules/@angular/cli/bin/ng build --configuration production",
    "*********TEST********": "*******************",
    "test": "ng test --configuration test",
    "test:watch": "ng test --configuration test --watch",
    "*********LINT********": "*******************",
    "pipeline-lint": "ng lint",
    "lint": "ng lint && pnpm run --prefix src/framework ng lint",
    "lint:fix": "ng lint -- --fix && pnpm run --prefix src/framework ng lint -- --fix",
    "sass-lint": "sass-lint --verbose --no-exit -c ./.sasslintrc",
    "sass-lint:fix": "sass-lint-auto-fix -d --config-sass-lint ./.sasslintrc -c ./.saaslintautofix",
    "*********TRANSLATIONS********": "*******************",
    "extract-translations": "npm run extract-translations-core && npm run extract-translations-app && npm run extract-translations-shared && npm run extract-translations-template && npm run extract-translations-home",
    "extract-translations-core": "ngx-translate-extract  --input ./src/Framework/core/ ./src/app/not-found/ --output  ./src/Framework/assets/i18n/core/en.json ./src/Framework/assets/i18n/core/en-GB.json ./src/Framework/assets/i18n/core/en-AU.json ./src/Framework/assets/i18n/core/fr-FR.json ./src/Framework/assets/i18n/core/nb.json ./src/Framework/assets/i18n/core/es-MX.json ./src/Framework/assets/i18n/core/nl.json ./src/Framework/assets/i18n/core/de-DE.json --clean --sort --format namespaced-json ",
    "extract-translations-app": "ngx-translate-extract  --input  ./src/translate/  --output  ./src/assets/i18n/app/en.json ./src/assets/i18n/app/en-GB.json ./src/assets/i18n/app/en-AU.json ./src/assets/i18n/app/fr-FR.json ./src/assets/i18n/app/nb.json ./src/assets/i18n/app/es-MX.json ./src/assets/i18n/app/nl.json ./src/assets/i18n/app/de-DE.json --clean --sort --format namespaced-json ",
    "extract-translations-shared": "ngx-translate-extract  --input  ./src/Framework/shared/  --output  ./src/Framework/assets/i18n/shared/en.json ./src/Framework/assets/i18n/shared/en-GB.json ./src/Framework/assets/i18n/shared/en-AU.json ./src/Framework/assets/i18n/shared/fr-FR.json ./src/Framework/assets/i18n/shared/nb.json ./src/Framework/assets/i18n/shared/es-MX.json ./src/Framework/assets/i18n/shared/nl.json ./src/Framework/assets/i18n/shared/de-DE.json --clean --sort --format namespaced-json ",
    "extract-translations-template": "ngx-translate-extract --input ./src/app/template/ --output ./src/assets/i18n/template/en.json ./src/assets/i18n/template/en-GB.json ./src/assets/i18n/template/en-AU.json ./src/assets/i18n/template/fr-FR.json ./src/assets/i18n/template/nb.json ./src/assets/i18n/template/es-MX.json ./src/assets/i18n/template/nl.json ./src/assets/i18n/template/de-DE.json --clean --sort --format namespaced-json ",
    "extract-translations-home": "ngx-translate-extract --input ./src/app/home/ --output ./src/assets/i18n/home/en.json ./src/assets/i18n/home/en-GB.json ./src/assets/i18n/home/en-AU.json ./src/assets/i18n/home/fr-FR.json ./src/assets/i18n/home/nb.json ./src/assets/i18n/home/es-MX.json ./src/assets/i18n/home/nl.json ./src/assets/i18n/home/de-DE.json --clean --sort --format namespaced-json ",
    "*********CONFIGURATION********": "*******************",
    "opency": "./node_modules/.bin/cypress open --project e2e --config-file cypress/cypress.json",
    "runcy": "./node_modules/.bin/cypress run --project e2e",
    "preinstall": "npx only-allow pnpm && ng config -g cli.packageManager pnpm",
    "postinstall": "ngcc"
  },
