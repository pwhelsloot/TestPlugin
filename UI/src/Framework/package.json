  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test --configuration test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "extract-translations-app": "ngx-translate-extract  --input  ./translate/  --output  ./assets/i18n/app/en.json ./assets/i18n/app/en-GB.json  ./assets/i18n/app/en-AU.json ./assets/i18n/app/fr-FR.json ./assets/i18n/app/nb.json ./assets/i18n/app/es-MX.json ./assets/i18n/app/nl.json ./assets/i18n/app/de-DE.json --clean --sort --format namespaced-json ",
    "extract-translations-core": "ngx-translate-extract  --input  ./core/ --output  ./assets/i18n/core/en.json ./assets/i18n/core/en-GB.json ./assets/i18n/core/en-AU.json ./assets/i18n/core/fr-FR.json ./assets/i18n/core/nb.json ./assets/i18n/core/es-MX.json ./assets/i18n/core/nl.json ./assets/i18n/core/de-DE.json --clean --sort --format namespaced-json ",
    "extract-translations-shared": "ngx-translate-extract  --input  ./shared/  --output  ./assets/i18n/shared/en.json ./assets/i18n/shared/en-GB.json ./assets/i18n/shared/en-AU.json ./assets/i18n/shared/fr-FR.json ./assets/i18n/shared/nb.json ./assets/i18n/shared/es-MX.json ./assets/i18n/shared/nl.json ./assets/i18n/shared/de-DE.json --clean --sort --format namespaced-json ",
    "extract-all-translations": "npm run extract-translations-app && npm run extract-translations-core && npm run extract-translations-shared",
    "build-high-mem": "node --max_old_space_size=10000 node_modules/@angular/cli/bin/ng build --configuration production",
    "sass-lint": "sass-lint --verbose --no-exit -c ./.sasslintrc",
    "sass-lint:fix": "sass-lint-auto-fix -d --config-sass-lint ./.sasslintrc -c ./.saaslintautofix",
    "preinstall": "npx only-allow pnpm && ng config -g cli.packageManager pnpm",
    "postinstall": "ngcc",
    "docs:json": "compodoc --disablePrivate --disableInternal --disableProtected --disableLifeCycleHooks -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6007",
    "build-storybook": "npm run docs:json && build-storybook"
