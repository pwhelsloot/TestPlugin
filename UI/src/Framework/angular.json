{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "platform-ui-framework": {
      "root": "",
      "sourceRoot": "./",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "stylePreprocessorOptions": {
              "includePaths": ["./node_modules/"]
            },
            "preserveSymlinks": true,
            "outputPath": "dist",
            "index": "index.html",
            "main": "main.ts",
            "tsConfig": "tsconfig.app.json",
            "polyfills": "polyfills.ts",
            "styles": [
              "sass/styles.scss",
              "node_modules/snazzy-info-window/dist/snazzy-info-window.scss",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/leaflet-draw/dist/leaflet.draw.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": true,
                "fonts": false
              },
              "outputHashing": "all",
              "progress": false,
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "hidden": true,
                "vendor": false
              },
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "preserveSymlinks": true,
            "main": "test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "sass/styles.scss",
              "node_modules/snazzy-info-window/dist/snazzy-info-window.scss",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/leaflet-draw/dist/leaflet.draw.css"
            ],
            "assets": ["./assets"]
          },
          "configurations": {
            "test": {
              "include": [
                "shared/**/*.spec.ts",
                "core/**/*.spec.ts",
                "auth/**/*.spec.ts",
                "translate/*.spec.ts"
              ]
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [".//**/*.ts", ".//**/*.html"]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "browserTarget": "platform-ui-framework:build",
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "browserTarget": "platform-ui-framework:build"
          }
        }
      }
    }
  },
  "defaultProject": "platform-ui-framework",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "defaultCollection": "@angular-eslint/schematics",
    "cache": {
      "enabled": true
    },
    "analytics": false
  }
}
